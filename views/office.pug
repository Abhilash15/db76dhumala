extends layout

block content
  h1= title
      
  div(class="container-fluid")
      div(class="row")
          -
            let vegetables = results
          each vegetable in vegetables
            div(class="col-sm-2") #{vegetable.department}
            div(class="col-sm-2") #{vegetable.size}
            div(class="col-sm-2") #{vegetable.strength}
